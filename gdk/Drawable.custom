/// <summary> Size property </summary>
/// <remarks> Returns the size of the Drawble </remarks>
public System.Drawing.Size Size {
    get {
        int x, y;
        GetSize(ref x, ref y);
        return new System.Drawing.Size(x, y);
    }
}

