/// <summary>
/// ToString method
/// </summary>
///
/// <remarks>
/// returns a string representation of this color
///
/// </remarks>

public override string ToString ()
{
	return String.Format ("rgb:{0:x}/{1:x}/{2:x}", red, green, blue);
}
