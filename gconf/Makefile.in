SUBDIRS = GConf GConf.PropertyEditors tools

@ENABLE_GNOME_TRUE@ all: linux
@ENABLE_GNOME_FALSE@ all: 

linux:
	for i in $(SUBDIRS); do \
		$(MAKE) -C $$i || exit 1; \
	done

install: all
	for i in $(SUBDIRS); do \
		$(MAKE) -C $$i install || exit 1; \
	done

clean:
	for i in $(SUBDIRS); do \
		$(MAKE) -C $$i clean || exit 1; \
	done
