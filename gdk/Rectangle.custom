/// <summary>
/// ToString method
/// </summary>
///
/// <remarks>
/// returns a string representation of this Rectangle
///
/// </remarks>

public override string ToString ()
{
	return String.Format ("{0}x{1}+{2}+{3}", width, height, x, y);
}


